---
import Layout from "@layouts/Layout.astro";
import { getI18N } from '@i18n/index'
import CatalogoComponent from "@components/Catalogo/Catalogo.astro";
const { currentLocale } = Astro
const i18n = getI18N({ currentLocale })
---

